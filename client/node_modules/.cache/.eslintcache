r add post form\n  const [newPost, setNewPost] = useState({\n    title: \"\",\n    description: \"\",\n    url: \"\",\n    status: \"TO LEARN\",\n  });\n  const { title, description, url, status } = newPost;\n  const onChangeNewPostForm = (event) => {\n    console.log(event.target.value);\n    setNewPost({ ...newPost, [event.target.name]: event.target.value });\n  };\n  //when click outside modal, close modal and clear form\n  const resetAddPostData = () => {\n    setNewPost({ title: \"\", description: \"\", url: \"\", status: \"TO LEARN\" });\n    setShowAddPostModal(false);\n  };\n  const onSubmit = async (event) => {\n    event.preventDefault();\n    const { success, message } = await addPost(newPost);\n    resetAddPostData();\n  };\n\n  return (\n    //onHide to close modal when you click outside the modal\n    <Modal show={showAddPostModal} onHide={resetAddPostData}>\n      <Modal.Header closeButton>\n        <Modal.Title>Great! Your step begins here</Modal.Title>\n      </Modal.Header>\n      <Form onSubmit={onSubmit}>\n        <Modal.Body>\n          <Form.Group>\n            <Form.Control\n              type=\"text\"\n              placeholder=\"Title\"\n              name=\"title\"\n              required\n              aria-describedby=\"title-help\"\n              value={title}\n              onChange={onChangeNewPostForm}\n            />\n            <Form.Text id=\"title-help\" muted>\n              Required\n            </Form.Text>\n          </Form.Group>\n          <Form.Group>\n            <Form.Control\n              as=\"textarea\"\n              rows={3}\n              placeholder=\"Description\"\n              name=\"description\"\n              value={description}\n              onChange={onChangeNewPostForm}\n            />\n          </Form.Group>\n          <Form.Group className=\"mt-2\">\n            <Form.Control\n              type=\"text\"\n              placeholder=\"Website's URL\"\n              name=\"url\"\n              value={url}\n              onChange={onChangeNewPostForm}\n            />\n          </Form.Group>\n        </Modal.Body>\n        <Modal.Footer>\n          <Button variant=\"secondary\" onClick={resetAddPostData}>\n            Cancel\n          </Button>\n          <Button variant=\"primary\" type=\"submit\">\n            Add to dashboard\n          </Button>\n        </Modal.Footer>\n      </Form>\n    </Modal>\n  );\n}\n\nexport default AddPostModal;\n","/Users/hungnguyen/Documents/CODE/REACT-PROJECTS/fullstack-learnapp/client/src/components/layout/UpdateModal.js",["93","94"],[],{"ruleId":"95","severity":1,"message":"96","line":29,"column":29,"nodeType":"97","endLine":29,"endColumn":31,"suggestions":"98"},{"ruleId":"99","severity":1,"message":"100","line":9,"column":59,"nodeType":"101","messageId":"102","endLine":9,"endColumn":67},{"ruleId":"99","severity":1,"message":"103","line":19,"column":36,"nodeType":"101","messageId":"102","endLine":19,"endColumn":42},{"ruleId":"99","severity":1,"message":"104","line":31,"column":13,"nodeType":"101","messageId":"102","endLine":31,"endColumn":20},{"ruleId":"99","severity":1,"message":"105","line":31,"column":22,"nodeType":"101","messageId":"102","endLine":31,"endColumn":29},{"ruleId":"99","severity":1,"message":"104","line":37,"column":13,"nodeType":"101","messageId":"102","endLine":37,"endColumn":20},{"ruleId":"99","severity":1,"message":"105","line":37,"column":22,"nodeType":"101","messageId":"102","endLine":37,"endColumn":29},"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'getPosts'. Either include it or remove the dependency array.","ArrayExpression",["106"],"no-unused-vars","'getPosts' is assigned a value but never used.","Identifier","unusedVar","'status' is assigned a value but never used.","'success' is assigned a value but never used.","'message' is assigned a value but never used.",{"desc":"107","fix":"108"},"Update the dependencies array to be: [getPosts]",{"range":"109","text":"110"},[1072,1